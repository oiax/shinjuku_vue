<div class="page-header">
  <h1>ユーザー登録情報の編集</h1>
</div>

<div class="panel panel-default">
  <div class="panel-body">
    <%= form_for @user do |f| %>
      <%= render "fields", f: f %>
      <%= f.submit "更新", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

<%= javascript_pack_tag "users/form" %>
